<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.collioni.douglas.seletortemas" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TemasActivity"
            android:label="@string/title_activity_temas"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.collioni.douglas.seletortemas.MainActivity" />
        </activity>
        <activity
            android:name=".FinalActivity"
            android:label="@string/title_activity_final"
            android:parentActivityName=".TemasActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.collioni.douglas.seletortemas.TemasActivity" />
        </activity>
        <activity
            android:name=".AvaliarActivity"
            android:label="@string/title_activity_avaliar" >
        </activity>
    </application>

</manifest>
